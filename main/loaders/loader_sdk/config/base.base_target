[platform]
platform = yandex_games
sdk_go = yandex_games_sdk

[input]
use_accelerometer = 0

[display]
width = 1920
height = 1080
high_dpi = 1
vsync = 0
display_profiles = /main/render/my.display_profilesc

[script]
shared_state = 1

[windows]
app_icon = /assets/app_icons/appicon.ico

[osx]
app_icon = /assets/app_icons/appicon.icns

[android]
input_method = HiddenInputField

[physics]
gravity_y = -1000.0
scale = 0.01
max_collisions = 1024
max_contacts = 512
trigger_overlap_capacity = 256

[tilemap]
max_tile_count = 30000
max_count = 1024

[graphics]
max_draw_calls = 4096

[sprite]
max_count = 4096
subpixels = 1

[factory]
max_count = 2048

[collectionfactory]
max_count = 1024

[collection]
max_instances = 2048

[model]
max_count = 128

[html5]
show_fullscreen_button = 0
show_made_with_defold = 0
scale_mode = stretch
heap_size = 128

[sound]
max_component_count = 256
max_sound_data = 512
max_sound_buffers = 256

[druid]
no_auto_template = 1
no_auto_input = 1

[label]
subpixels = 1

[resource]
max_resources = 2048

[engine]
fixed_update_frequency = 30

[share_log]
block_release_version = false
catch_promise_errors = true
force_html5_init = true

[gui]
max_count = 128

